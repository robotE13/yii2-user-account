{
    "name": "robote13/yii2-user-account",
    "description": "Yii2 implementation of robote13/user-account",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "user"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Dudal",
            "email": "wolfstrace@gmail.com"
        }
    ],
    "minimum-stability" : "dev",
    "require": {
        "php": "^7.4 | ^8",
        "yiisoft/yii2": "~2.0.14",
        "robote13/user-account": "^0.1.0 | ~0.2.0-alpha"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/verify": "^1.3.0",
        "codeception/specify": "^1.3.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "git@github.com:robotE13/user-account.git"
        },
        {
            "type": "package",
            "package": {
                "name": "robote13/user-account",
                "version": "0.2.0-alpha",
                "dist": {
                    "url": "/packages/user-account",
                    "type": "path"
                }
            }
        }
    ],
    "scripts": {
        "codecept": [
            "codecept"
        ],
        "test": [
            "codecept -v run"
        ]
    },
    "autoload": {
        "psr-4": {
            "RobotE13\\Yii2UserAccount\\": "src"
        }
    },
    "extra": {
        "bootstrap": "RobotE13\\Yii2UserAccount\\Bootstrap"
    }
}
